#include <stdio.h>
#include <stdlib.h>

int main() {
    int n1, n2;

    printf("\nEndereco da variavel 1: %p\n",&n1);
    printf("\nEndereco da variavel 2: %p\n",&n2);

    if(&n1>&n2){
        printf("\nMaior endereco: %p\n",&n1);
        }else{
        printf("\Maior endereco: %p\n",&n2);
    }
    return 0;
}
