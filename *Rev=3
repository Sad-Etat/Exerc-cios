#include <stdio.h>
#include <stdlib.h>

int main() {
    int v1, v2;

    printf("\nEscreva o valor de v1: \n");
    scanf("%i",&v1);
    printf("\nEscreva o valor de v2: \n");
    scanf("%i",&v2);
    printf("\nEndereco da variavel 1: %p\n",&v1);
    printf("\nEndereco da variavel 2: %p\n",&v2);

    if(&v1>&v2){
        printf("\nMaior endereco: %p\n",&v1);
        }else{
        printf("\Maior endereco: %p\n",&v2);
    }
    return 0;
}
