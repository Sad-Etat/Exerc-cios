#include <stdio.h>
#include <stdlib.h>

int main()
{
    int v[10];
    int i=1;
    int j=0;
    while(j<10){
        printf("\n Escreva o %i numero:\n",i);
        scanf("%i",&v[i]);
        j++;
        i++;
    }
    i=10;
    while(j>0){
        printf("\n%i",v[i]);
        i--;
        j--;
    }
    return 0;
}
