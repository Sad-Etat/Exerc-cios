#include <stdio.h>
#include <stdlib.h>

int multiplo(int n1)
{
    int n1;
    if (n1/4!=int)
        printf("\nE multiplo de 4\n");
    else
        printf("\nNao e multiplo de 4\n");
}


int main()
{
    int n1;
    printf("\nDigite um numero\n");
    scanf("%i",&n1);
    multiplo(n1);

    return 0;
}
